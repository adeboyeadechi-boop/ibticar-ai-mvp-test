{
  "name": "ibticar-ai-mvp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/start-with-db-check.mjs",
    "dev:unsafe": "next dev",
    "build": "npm run db:deploy && next build",
    "build:local": "npx prisma generate && next build",
    "postinstall": "npx prisma generate",
    "start": "node scripts/start-with-db-check.mjs",
    "start:unsafe": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"src/**/*.@(js|jsx|ts|tsx|json|css|md)\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:check": "node scripts/init-db.mjs",
    "db:seed": "npx dotenv -e .env -- tsx prisma/seed.ts",
    "db:migrate": "npx prisma migrate dev",
    "db:migrate:deploy": "npx prisma migrate deploy",
    "db:deploy": "npx prisma migrate deploy && npx prisma db seed",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@auth/prisma-adapter": "^2.11.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@types/jsonwebtoken": "^9.0.10",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.553.0",
    "next": "16.0.1",
    "next-auth": "^4.24.13",
    "node-fetch": "^2.7.0",
    "qrcode": "^1.5.4",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "speakeasy": "^2.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^6.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/speakeasy": "^2.0.10",
    "babel-plugin-react-compiler": "1.0.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "jest-environment-node": "^30.2.0",
    "jest-mock-extended": "^4.0.0",
    "prettier": "^3.6.2",
    "prisma": "^6.19.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.5",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
